{
  "presets": [
    "@babel/react",
    "@babel/typescript",
    ["@babel/env", { "modules": false }]
  ],
  "plugins": [
    "@babel/plugin-proposal-object-rest-spread",
    "@babel/plugin-transform-async-to-generator"
  ],
  "env": {
    "test": {
      "presets": ["@babel/react", ["@babel/env", { "modules": "commonjs" }]]
    }
  }
}
